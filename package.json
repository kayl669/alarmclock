{
    "name": "alarm-clock",
    "version": "0.2.0",
    "description": "Alarm clock written in NodeJS.",
    "author": "Sebastiaan Luca",
    "license": "MIT",
    "private": true,
    "main": "src/app.js",
    "dependencies": {
        "axios": "0.19.2",
        "bindings": "1.5.0",
        "body-parser": "1.19.0",
        "config": "3.3.0",
        "cors": "2.8.5",
        "debug": "4.1.1",
        "electron": "^9.0.3",
        "electron-reload": "^1.5.0",
        "express": "4.17.1",
        "express-urlrewrite": "1.2.0",
        "fs-readfile-promise": "3.0.1",
        "hbs": "4.1.0",
        "lodash": "4.17.15",
        "loudness": "0.3.0",
        "moment": "2.24.0",
        "nocache": "^2.1.0",
        "node-schedule": "1.3.2",
        "request": "2.88.2",
        "rpi-wifi-connection": "^1.0.14",
        "socket.io": "2.3.0",
        "socket.io-client": "2.3.0",
        "speaker": "0.5.1",
        "uws": "100.0.1"
    },
    "devDependencies": {
        "babel-core": "6.26.3",
        "babel-loader": "7.1.5",
        "babel-plugin-transform-async-to-generator": "6.24.1",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "path": "0.12.7",
        "webpack": "4.42.0",
        "webpack-cli": "3.3.11"
    },
    "scripts": {
        "dev": "node_modules/webpack/bin/webpack.js --progress --hide-modules",
        "build": "node_modules/webpack/bin/webpack.js --progress --hide-modules -p",
        "debug": "DEBUG=alarm:* node alarm.js",
        "test": "yarn run dev && clear && yarn run debug",
        "run-debug": "yarn run dev && clear && DEBUG=alarm:* node alarm.js",
        "run": "clear && node alarm.js",
        "restart": "sudo supervisorctl restart alarm",
        "start-electron": "electron ./main.js --disable-gpu"
    }
}
